{
    "Mexico": {
        "INDC\u8a55\u8ad6": "\u58a8\u897f\u54e5\u662f\u5168\u4e16\u754c\u7b2c\u4e00\u500b\u4ea4\u51faINDC\u7684\u958b\u767c\u4e2d\u570b\u5bb6\uff0c\u9996\u958b\u958b\u767c\u4e2d\u570b\u5bb6\u63d0\u51fa\u6e1b\u78b3\u76ee\u6a19\u7684\u5148\u4f8b\u9577\u671f\u76ee\u6a19\u4e0a\u5247\u4f9d\u64da\u5176\u570b\u5167\u6c23\u5019\u8b8a\u9077\u5c08\u6cd5(LGCC)\uff0c\u5c07\u57282050\u5e74\u4e4b\u524d\uff0c\u76f8\u8f03\u65bc2000\u5e74\u7684\u6c34\u6e96\u6e1b\u639250%\u3002", 
        "\u6e1b\u78b3\u76ee\u6a19": "2026\u5e74\u9054\u78b3\u6392\u5cf0\u503c\uff1b\u76f8\u8f03\u65bc2013\u5e74\u7684BAU\uff0c2030\u5e74\u81ea\u4e3b\u6e1b\u639222%"
    }, 
    "United States of America": {
        "INDC\u8a55\u8ad6": "\u7f8e\u570b\u662f\u4e16\u754c\u6eab\u5ba4\u6c23\u9ad4\u6392\u653e\u91cf\u7b2c\u4e8c\u540d\u7684\u570b\u5bb6\uff0c\u4ed6\u5011\u8a02\u5b9a2050\u5e74\u524d\u81f3\u5c11\u8981\u524a\u6e1b80%\u7684\u9577\u671f\u76ee\u6a19\u3002\u7f8e\u570b\u81ea\u5f9e\u812b\u96e2\u4eac\u90fd\u8b70\u5b9a\u66f8\u4ee5\u4f86\uff0c\u6d88\u6975\u7684\u6696\u5316\u5c0d\u7b56\u98fd\u53d7\u6279\u8a55\uff0c\u53bb\u5e7411\u6708\u6b50\u5df4\u99ac\u7e3d\u7d71\u8868\u660e\u4e86\u524a\u6e1b\u76ee\u6a19\u4e26\u5c55\u73fe\u4ed6\u7684\u7a4d\u6975\u614b\u5ea6\uff0c\u9664\u4e86\u5728\u53bb\u5e74\u7684APEC\u4e0a\u548c\u7fd2\u8fd1\u5e73\u5171\u540c\u767c\u8868\u4e86\u6c23\u5019\u8072\u660e\uff0c\u5728\u4e09\u6708\u5e95\uff0c\u6b50\u5df4\u99ac\u4e5f\u5ba3\u5e03\u806f\u90a6\u653f\u5e9c\u672c\u8eab\u8981\u57282025\u5e74\u76f8\u8f03\u65bc2005\u5e74\u7684\u6c34\u6e96\u6e1b\u639240%\uff0c\u70ba\u570b\u5167\u5176\u4ed6\u6392\u78b3\u90e8\u9580\u4f5c\u51fa\u793a\u7bc4\uff0c\u5e0c\u671b\u53ef\u4ee5\u62ff\u5230COP21\u4ea4\u6d89\u7684\u4e3b\u5c0e\u6b0a\u3002", 
        "\u6e1b\u78b3\u76ee\u6a19": "\u76f8\u8f03\u65bc2005\u5e74\u6392\u78b3\u6c34\u6e96\uff0c2020\u5e74\u6e1b\u639217%(\u7b49\u540c\u76f8\u8f03\u65bc1990\u5e74\u6c34\u6e96\u6e1b\u63924%)\uff0c2025\u5e74\u6e1b\u639226-28%(\u7b49\u540c\u76f8\u8f03\u65bc1990\u5e74\u6c34\u6e96\u6e1b\u639214-17%)"
    }, 
    "EU": {
        "INDC\u8a55\u8ad6": "\u6b50\u76df\u4ee5\u5340\u57df\u7d44\u7e54\u7684\u8eab\u5206\u63d0\u51fa\u4e86INDC\uff0c\u4e26\u4e14\u5f37\u8abf\u6240\u6709\u6e1b\u78b3\u90fd\u5c07\u5728\u5883\u5167\u81ea\u4e3b\u9054\u6210\uff0c\u4e0d\u6703\u5c07\u570b\u969b\u6a5f\u5236\u7d0d\u5165\u6e1b\u78b3\u8a08\u7b97\u4e2d\u3002\u7136\u800c\u5728\u571f\u5730\u4f7f\u7528\u8207\u53e2\u6797\u8a08\u7b97\u898f\u5247\u8207\u4f54\u6bd4\u7684\u90e8\u5206\u76f8\u7576\u6a21\u7cca\uff0c\u4f7f\u6b50\u76df\u7684\u6e1b\u78b3\u76ee\u6a19\u6253\u4e86\u6298\u6263\u3002", 
        "\u6e1b\u78b3\u76ee\u6a19": "\u76f8\u8f03\u65bc1990\u5e74\u6392\u78b3\u6c34\u6e96\uff0c2030\u5e74\u6e1b\u639240%"
    }, 
    "Switzerland": {
        "INDC\u8a55\u8ad6": "\u745e\u58eb\u6436\u5148\u5168\u7403\u7e73\u4ea4\u4e86INDC\uff0c\u5728\u8cc7\u8a0a\u516c\u958b\u900f\u660e\u7684\u90e8\u5206\u4e0a\u76f8\u7576\u5b8c\u6574\u3002\u7136\u800c\u6839\u64daIPCC\u7684\u5efa\u8b70\uff0c\u745e\u58eb\u7684\u6e1b\u78b3\u627f\u8afe\u50c5\u9054\u5230\u5176\u6e1b\u78b3\u80fd\u529b\u8207\u8cac\u4efb\u7684\u6700\u4f4e\u6a19\u6e96\u800c\u5df2\u3002\u6b64\u5916\uff0c\u745e\u58eb\u5728INDC\u4e2d\u63d0\u5230\u6703\u5c07\u571f\u5730\u4f7f\u7528\u8207\u53e2\u6797\u7686\u7d0d\u70ba\u6e1b\u78b3\u6a19\u7684\u8a08\u7b97\uff0c\u52a0\u4e0a\u90e8\u5206\u6e1b\u78b3\u5c07\u65bc\u5883\u5916\u9054\u6210\uff0c\u610f\u5473\u8457\u5176\u5be6\u745e\u58eb\u7684\u570b\u5167\u81ea\u4e3b\u6e1b\u78b3\u5c07\u5360\u5176\u63d0\u51fa\u7684\u6e1b\u78b3\u76ee\u6a19\u76f8\u5c0d\u4e0d\u591a\u3002", 
        "\u6e1b\u78b3\u76ee\u6a19": "\u76f8\u8f03\u65bc1990\u5e74\u6392\u78b3\u6c34\u6e96\uff0c2025\u5e74\u6e1b\u639235%\uff0c2030\u5e74\u6e1b\u639250%\uff0c2050\u5e74\u6e1b\u639270-85%\u7684\u6e1b\u78b3\u76ee\u6a19"
    }, 
    "Norway": {
        "INDC\u8a55\u8ad6": "\u632a\u5a01\u5728LULUCF\u7684\u8b70\u984c\u4e0a\uff0c\u660e\u78ba\u7684\u9ede\u51fa\u4e0d\u8ad6\u6700\u5f8cLULUCF\u7684\u8a08\u7b97\u65b9\u5f0f\u70ba\u4f55\uff0c\u90fd\u4e0d\u6703\u5f71\u97ff\u632a\u5a01\u57282030\u5e74\u76f8\u8f03\u65bc1990\u5e74\u6c34\u6e96\u6e1b\u5c11(\u5de5\u696d)\u6392\u78b340%\u7684\u91ce\u5fc3\uff0c\u4e26\u9032\u4e00\u6b65\u9810\u544a\u4e86\u672a\u4f86\u6b50\u76df\u53ef\u80fd\u662f\u4ee5\u4e00\u500b\u6574\u9ad4\u4f86\u63d0\u4ea4\u5176\u6e1b\u78b3\u6210\u679c\uff0c\u800c\u975e\u7531\u5404\u570b\u5206\u5225\u8a08\u7b97\uff0c\u4e5f\u56e0\u6b64\u632a\u5a01\u5728INDC\u4e2d\u4e5f\u5f37\u8abf\u5176\u5c0d\u65bc\u5728\u5df4\u9ece\u5354\u8b70\u4e2d\u5efa\u7acb\u5e02\u5834\u6a5f\u5236\u7684\u91cd\u8981\u6027\uff0c\u4e26\u4e0d\u6392\u9664\u6cbf\u7528\u904e\u53bb\u4eac\u90fd\u8b70\u5b9a\u66f8\u78ba\u7acb\u7684\u5171\u540c\u57f7\u884c\u3001\u6e05\u6f54\u767c\u5c55\u6a5f\u5236\u3002", 
        "\u6e1b\u78b3\u76ee\u6a19": "\u76f8\u8f03\u65bc1990\u5e74\u6392\u78b3\u6c34\u6e96\uff0c2030\u5e74\u6e1b\u639240%"
    }, 
    "Russia": {
        "INDC\u8a55\u8ad6": "\u4fc4\u7f85\u65af\u5c0d\u65bc\u8abf\u9069\u8a08\u756b\u4e5f\u662f\u96bb\u5b57\u672a\u63d0\uff0c\u9664\u4e86\u4fc4\u7f85\u65af\u5317\u90e8\u63a5\u58e4\u5317\u6975\uff0c\u4fc4\u7f85\u65af\u5883\u5167\u7684\u68ee\u6797\u9762\u7a4d\u4e5f\u4f54\u5168\u4e16\u754c\u768425%\uff0c\u5728\u8abf\u9069\u4e0a\u537b\u6c92\u6709\u4efb\u4f55\u660e\u78ba\u653f\u7b56\uff0c\u4f7f\u5f97\u751f\u7269\u591a\u6a23\u6027\u53ca\u4eba\u6c11\u61c9\u5c0d\u6975\u7aef\u6c23\u5019\u7684\u61c9\u8b8a\u80fd\u529b\u76f8\u7576\u582a\u6182\u3002", 
        "\u6e1b\u78b3\u76ee\u6a19": "\u76f8\u8f03\u65bc1990\u5e74\u6392\u78b3\u6c34\u6e96\uff0c2025\u5e74\u6e1b\u639225-30%"
    }
}